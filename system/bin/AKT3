sleep 5; main_menu_op3;
exit;;
5 ) clear
echo "Coming Soon"
echo "You tell me what others tweaks you want me too add"
sleep 5; main_menu_op3;
exit;;
6 ) clear; main_menu;;
* ) echo " Please, enter a valid option..."
sleep 2
esac
# }
 done
